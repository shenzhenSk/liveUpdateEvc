{
  "name": "osp-frontend-vue3",
  "version": "0.0.30",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "npm run build:version && run-p type-check build-only",
    "build:testing": "vite build --mode testing",
    "build:version": "node ./vue.version.js",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build-only": "vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .eslintignore",
    "format": "prettier --write src/",
    "postinstall": "husky install && npm run lint"
  },
  "dependencies": {
    "@ionic/portals": "^0.8.1",
    "axios": "^1.3.4",
    "js-cookie": "^3.0.1",
    "pinia": "^2.0.32",
    "superapp-miniapp-invoke-native": "^1.0.84",
    "vant": "^4.1.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/core": "^5.2.2",
    "@hapi/eslint-plugin": "^6.0.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@testing-library/dom": "^9.3.1",
    "@types/js-cookie": "^3.0.3",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@types/postcss-pxtorem": "^6.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "less": "^4.1.3",
    "mockjs": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.4",
    "rollup-plugin-visualizer": "^5.9.0",
    "serve": "^14.2.0",
    "start-server-and-test": "^2.0.0",
    "terser": "^5.16.5",
    "time-stamp": "^2.2.0",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vconsole": "^3.15.1",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.2.0"
  }
}
